<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bugstack.infrastructure.dao.IRuleTreeNodeLineDao">

    <resultMap id="DataMap" type="cn.bugstack.infrastructure.dao.po.RuleTreeNodeLine">
        <id column="id" property="id"/>
        <id column="tree_id" property="treeId"/>
        <id column="rule_node_from" property="ruleNodeFrom"/>
        <id column="rule_node_to" property="ruleNodeTo"/>
        <id column="rule_limit_type" property="ruleLimitType"/>
        <id column="rule_limit_value" property="ruleLimitValue"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

<!--    RuleTreeNodeLine queryRuleTreeNodeLineByTreeId(String treeId);-->
    <select id="queryRuleTreeNodeLineByTreeId" parameterType="java.lang.String" resultMap="DataMap">
        select tree_id, rule_node_from, rule_node_to, rule_limit_type, rule_limit_value
        from rule_tree_node_line
        where tree_id = #{treeId}
    </select>
</mapper>
